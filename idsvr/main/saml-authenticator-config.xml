<config xmlns="http://tail-f.com/ns/config/1.0">
    <profiles xmlns="https://curity.se/ns/conf/base">
    <profile>
    <id>authentication-service</id>
    <type xmlns:auth="https://curity.se/ns/conf/profile/authentication">auth:authentication-service</type>
      <settings>
      <authentication-service xmlns="https://curity.se/ns/conf/profile/authentication">
      <authenticators>
      <authenticator>
        <id>saml</id>
        <saml2 xmlns="https://curity.se/ns/conf/authenticators/saml2">
          <issuer-entity-id>federation-client</issuer-entity-id>
          <clock-skew>61</clock-skew>
          <authentication-context-class-reference>
            <explicit>urn:se:curity:authentication:html-form:Username-Password2</explicit>
          </authentication-context-class-reference>
          <force-authn>if-requested-by-client</force-authn>
          <idp-entity-id>com.example</idp-entity-id>
          <idp-url>https://provider3.example.com:8446/authn/authentication</idp-url>
          <request-options>
            <nameid-format>urn:oasis:names:tc:SAML:1.1:nameid-format:unspecified</nameid-format>
          </request-options>
          <signature-verification-key>provider3-signature-verification-key</signature-verification-key>
          <wants-response-signed>true</wants-response-signed>
          <wants-assertion-signed>false</wants-assertion-signed>
          <request-signing-key>default-signing-key</request-signing-key>
        </saml2>
      </authenticator>
      </authenticators>
      </authentication-service>
      </settings>
  </profile>
  </profiles>
